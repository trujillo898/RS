local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")
local brainrotsFolder = workspace:WaitForChild("Brainrots")

while true do
    for _, brainrot in pairs(brainrotsFolder:GetChildren()) do
        if brainrot:IsA("Model") and brainrot:FindFirstChildWhichIsA("BasePart") then
            local part = brainrot:FindFirstChildWhichIsA("BasePart")
            if (part.Position - hrp.Position).Magnitude < 15 then
                firetouchinterest(hrp, part, 0)
                wait()
                firetouchinterest(hrp, part, 1)
            end
        end
    end
    wait(0.3)
end
